<link rel="stylesheet" href="/stylesheets/review.css">
<%- include('../partials/nav') %>

<section>
  <h1>List of Added Champions</h1>
  <% champions.forEach(champion => { %>
    <li>Champions: <%= champions.name %> </li>
    <form action="/champions/<%= champion._id %>?_method=DELETE" method="POST">
      <button class="btn" type="submit">x</button>
    </form>
  <% }) %>
</section>

