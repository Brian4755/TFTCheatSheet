  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
  <link rel="stylesheet" href="/stylesheets/main.css">
</head>
<body>
  <nav>
    <a href="/" class="container">
    <img class="imageIcon" src="/images/poro.png" alt="Poro Icon">
    </a>
    <% if (user) { %>
      <div class="dropdown">
        <button class="dropdown-button">
          +
        </button>
        <div class="dropdown-content">
          <a href="/reviews/new">Add Review</a>
          <a href="/champions/new">Add Champion</a>
          <a href="/champions">Delete Champion</a>
        </div>
      </div>
    <% } %>
    <a class="reviews" href="/reviews">Reviews</a>
    <a class="champions" href="">Champions</a>
    <% if (user) { %>
      <div class="dropdownTwo">
        <a class="logout" href="/auth/logout">Sign out</a>
        <div class="dropdown-contentTwo">
          <p>Signed in as <%= user.profile.name %></p>
        </div>
      </div>
    <% } else { %>
      <div id="loginFix" class="dropdown">
        <a class="reviews">
          Sign in
        </a>
        <div id="login" class="dropdown-content">
          <a href="/auth/google" class="siwg-link">
            <button type="submit" class="siwg">
              <img id="siwg-image" alt="Sign in with Google" />
            </button>
          </a>
        </div>
      </div>
    <% } %>
  </nav>
</body>
